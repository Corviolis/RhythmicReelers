config_version=5

[application]

config/name="Rhythmic Reelers"
config/version="1.0-prealpha"
run/main_scene="res://scenes/lobby/lobby.tscn"
config/features=PackedStringArray("4.3", "Forward Plus")
boot_splash/bg_color=Color(0.533333, 0.843137, 0.870588, 1)
boot_splash/use_filter=false
config/icon="res://icon.png"
config/windows_native_icon="res://icon-small.ico"
config/transparent_background=true

[audio]

buses/default_bus_layout="res://music/music_busses/default_bus_layout.tres"

[autoload]

MultiplayerInput="*res://addons/multiplayer_input/multiplayer_input.gd"
PlayerManager="*res://singletons/player_manager.gd"
NetworkManager="*res://singletons/network_manager.gd"
GlobalUtils="*res://singletons/global_utils.gd"
RhythmEngine="*res://singletons/rhythm_engine.gd"

[debug]

gdscript/warnings/integer_division=0

[display]

window/size/viewport_width=320
window/size/viewport_height=180
window/size/window_width_override=1280
window/size/window_height_override=720
window/stretch/mode="viewport"
window/stretch/scale_mode="integer"
window/vsync/vsync_mode=0

[dotnet]

project/assembly_name="Rhythmic Reelers"

[editor_plugins]

enabled=PackedStringArray("res://addons/beat_map/plugin.cfg", "res://addons/gut/plugin.cfg", "res://addons/multiplayer_input/plugin.cfg")

[filesystem]

import/blender/enabled=false
import/fbx/enabled=false

